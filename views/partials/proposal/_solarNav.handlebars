<style>
  @media only screen and (max-width: 993px){
     #aboutAes{
      margin-top: 30px;
    }
    .finishedArray{
      margin-top:30px;
    }
    .compareProposals{
      margin-top:20px;
    }
    .navbar {
      background-color: rgba(37, 79, 142, 1);
    } 
  }
  
  @media only screen and (min-width: 993px){

    #aboutAes{
      margin-top: 50px;
    }
    .finishedArray{
      margin-top: 50px;
    }
    .compareProposals{
      margin-top: 50px;
  }

  .navbar {
    
    height: 70px;
    padding: 0px;
    padding-top: 30px;
    background-color: rgb(228, 234, 241);
    box-shadow: 3px 3px 8px rgba(139, 139, 139, .2);
    border-bottom: 1px solid rgba(56, 56, 56, 0.6);
  }

  .navbar-nav li:first-child{
    margin-left: 20px;
  }

  .navbar-nav {
    position: absolute;
    top: 20px;
  }

  .navbar-brand,
  .navbar-nav li a{ 
      background-color: rgb(89, 116, 155);
      padding: 10px 20px;
      font-size: 18px;
      font-weight: 100;
      text-transform: uppercase;
      border-right: .8px solid rgba(56, 56, 56, 0.6);
      border-top: .8px solid rgba(56, 56, 56, 0.6);
      border-bottom: .8px solid rgba(56, 56, 56, 0.6);
      z-index: 1;
  }
  .navbar-nav li a.active { 

    background-color: rgba(37, 79, 142, 1);
    transform: translateY(3px) scaleX(1.1);
    margin-right: 5px;
    border-bottom: none;
    z-index: 10;
  }
  .navbar-nav li a:first-child{ 
    border-top-right-radius: 6px;
    border-left: .8px solid rgba(56, 56, 56, 0.6);
  }
  .navbar-nav li a:last-child { 
    border-top-left-radius: 5px;
  }

  .nav-link {
    color: #fff !important;
    padding: 10px 20px !important;
  }
}


</style>


<nav class="navbar fixed-top navbar-expand-lg navbar-dark bg-aes">
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup"
    aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
    <div class="navbar-nav">
      <li class="nav-item">
        <a class="nav-item nav-link active" data-id="aboutAes" data-toggle="collapse" href="#">What You Should Know </a>
      </li>
      {{#each finishedArray}}
      <li class="nav-item align-self-center">

      </li>
      <li class="nav-item">
        <a class="nav-link" data-id="{{proposal.proposal_title}}" data-toggle="collapse" href="#">{{{proposal.proposal_title}}}</a>
      </li>
      {{/each}}
      {{#if compareArray}}
      <li class="nav-item align-self-center">
      </li>
      <li class="nav-item">
        <a class="nav-item nav-link" data-id="compareProposal" data-toggle="collapse" href="#" id="compareNav">Compare
          Proposals</a>
      </li>
      {{/if}}


    </div>

  </div>


</nav>