<div class="page leading-relaxed text-gray-800" id="page_{{{_fid_3}}}">
  <div class="container mx-auto print:px-5 solar_storage_generator">
    {{> offering/layout/header}}
    <div class="flex -mt-2">
      {{! Left Side }}
      <div class="w-1/2">
        {{! Image and graph }}
        {{> offering/layout/kWh}}
        <div class="my-2 flex">
          {{#ifCon _fid_373 '!=' 'https://aes.quickbase.com/up/bpmbhwbng/a/r3150/e372/v0?'}}
            <img class="w-auto" src="{{{_fid_373}}}" style="max-height: 280px; max-width: 280px;" />
          {{/ifCon}}
        </div>
        <div class="flex justify-between">
          <a href="{{{solar._fid_272}}}">
            <img class="w-full" src="{{{solar._fid_198}}}" alt="Product Image"
              style="max-width: 180px;" />
          </a>
          <div class="w-8/12 flex flex-col mt-5 pl-2">
            <div class="w-full">
              <p class="font-bold mb-1">
                Equipment
              </p>
              <ul class="list-none text-xs">
                <li>
                  {{{solar._fid_155}}}
                </li>
                <li>
                  {{{solar._fid_194}}}
                </li>
                <li>
                  {{{solar._fid_196}}}
                </li>
                <li>
                  Monitoring:
                  {{{solar._fid_149}}}
                </li>
                {{#if hasSolarInsure}}
                  <li>
                    <a href="https://aes.quickbase.com/up/bkdfitsjr/a/r43379/e6/v0?Copy of SolarInsure Brochure plus certificate package.pdf"
                      class="text-blue-600">
                      SolarInsure
                    </a>
                  </li>
                {{/if}}
              </ul>
            </div>
          </div>
        </div>
      </div>
      {{! Right Side }}
      <div class="w-1/2 flex flex-col">
        {{!Place holder  }}
        <h3 class="font-bold text-xl">
          Battery Storage
        </h3>
        <div class="flex">
          <div class="w-3/12">
            <a href="{{{storage._fid_272}}}">
              <img class="mt-5" src="{{{storage._fid_198}}}" alt="Product Image" />
            </a>
          </div>
          <div class="w-8/12">
            <div class="flex items-center">
              <h3 class="text-4xl font-bold">
                {{{storage._fid_197}}}
              </h3>
              <p class="ml-4 leading-tight font-thin">
                Continuous power
                <br />
                <span class="ml-4">
                  during an outage
                </span>
              </p>
            </div>
            <div class="flex mt-2">
              <div class="w-6/12">
                <p class="font-bold mb-1">
                  Equipment
                </p>
                <ul class="list-none text-xs">
                  <li>
                    {{{storage._fid_155}}}
                  </li>
                  <li>
                    {{{storage._fid_47}}}
                  </li>
                </ul>
              </div>
              <div class="w-6/12">
                {{> offering/layout/offering/storage-backup}}
              </div>
            </div>
          </div>
        </div>
        <div class="border-b-2 border-gray-400 my-6"></div>
        {{! Head }}
        <div class="flex">
          <div class="w-3/12">
            <h3 class="font-bold text-xl">
              Generator
            </h3>
          </div>
          <div class="w-8/12 ml-2">
            <h3 class="text-4xl font-bold -mt-4">
              {{{generator._fid_143}}}
              <span class="text-xl font-thin">
                {{{generator._fid_257}}}
              </span>
            </h3>
          </div>
        </div>
        {{! #3 col }}
        {{> offering/layout/offering/generator-backup}}
        {{! 2col }}
        <div class="flex items-center justify-between">
          <a href="{{{generator._fid_272}}}">
            <img class="mt-5" src="{{{generator._fid_198}}}" alt="Product Image"
              style="max-width: 170px; max-height: 200px; height: auto;" />
          </a>
          <div class="w-7/12 mt-2">
            <p class="font-bold mb-1">
              Equipment
            </p>
            <ul class="list-none text-sm">
              <li>
                {{{generator._fid_155}}}
              </li>
              <li>
                {{{generator._fid_47}}}
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
