<div class="page leading-relaxed text-gray-800 print-scale-90" id="page_{{{_fid_3}}}">
  <div class="container mx-auto print:px-5">
    {{> offering/layout/header}}
    <div class="flex -mt-2">
      {{! Left Side }}
      <div class="w-1/2">
        {{! Image and graph }}
        <div>
          {{> offering/layout/kWh}}
          <div class="mt-5">
            <img src="{{{_fid_373}}}" alt="" style="max-width: 400px; height: auto;" />
          </div>
        </div>
      </div>
      {{! Right Side }}
      <div class="w-1/2 flex-1 flex flex-col">
        <div class="flex">
          <div class="mt-10">
            <a href="{{{solar._fid_272}}}">
              <img src="{{{solar._fid_198}}}" alt="Product Image"
                style="max-width: 170px; height: auto; max-height: 200px;" />
            </a>
          </div>
          <div class="ml-10 flex flex-col justify-between mt-5">
            <div class="w-full">
              <p class="font-bold mb-1">
                Equipment
              </p>
              <ul class="list-none text-sm">
                <li>
                  {{{solar._fid_155}}}
                </li>
                <li>
                  {{{solar._fid_194}}}
                </li>
                <li>
                  {{{solar._fid_196}}}
                </li>
                <li>
                  Monitoring:
                  {{{solar._fid_149}}}
                </li>
                {{#if hasSolarInsure}}
                  <li>
                    <a href="https://aes.quickbase.com/up/bkdfitsjr/a/r43379/e6/v0?Copy of SolarInsure Brochure plus certificate package.pdf"
                      class="text-blue-600">
                      SolarInsure
                    </a>
                  </li>
                {{/if}}
              </ul>
            </div>
          </div>
        </div>
        <div class="border-b-2 border-gray-400 my-6"></div>
        {{! Head }}
        <div class="flex justify-between">
          <div class="w-3/12">
            <h3 class="font-bold text-xl">
              Generator
            </h3>
          </div>
          <div class="w-9/12">
            <h3 class="text-5xl font-bold -mt-4">
              {{{generator._fid_143}}}
              <span class="text-xl font-thin">
                {{{generator._fid_257}}}
              </span>
            </h3>
          </div>
        </div>
        {{! #3 col }}
        {{> offering/layout/offering/generator-backup}}
        {{! 2col }}
        <div class="flex items-center mt-2">
          <a href="{{{generator._fid_272}}}">
            <img src="{{{generator._fid_198}}}" alt="Product Image" style="max-width: 180px;" />
          </a>
          <div class="ml-10">
            <p class="font-bold mb-1">
              Equipment
            </p>
            <ul class="list-none text-sm">
              <li>
                {{{generator._fid_155}}}
              </li>
              <li>
                {{{generator._fid_47}}}
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
