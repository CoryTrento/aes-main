        </sup>
      </th>
      {{#each pricing}}
        <td class="text-center text-xs">
          {{#if hasFinancing}}
            {{{_fid_818}}}
          {{/if}}
        </td>
      {{/each}}
    </tr> --}}
    <tr>
      <th class="text-right font-bold pr-4 text-xs">
        Net Financed Amount
      </th>
      {{#each pricing}}
        <td class="text-center text-xs font-bold">
          {{#if hasFinancing}}
            {{{toMoneyDec _fid_825}}}
          {{/if}}
        </td>
      {{/each}}
    </tr>
  </div>
{{/if}}
